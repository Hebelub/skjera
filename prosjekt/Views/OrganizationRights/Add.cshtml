@model IEnumerable<ApplicationUser>


<h2>Seach for user to Add To the Organization</h2>

<!--SearchUsers-->
<p1 class="container" style="text-align: left">
    <form asp-controller="OrganizationRights" asp-action="Add" method="get" asp-route-id="@ViewData["OrganizationId"]">
        <input type="text" placeholder="Users" name="SearchString"/>
        <button type="submit">Search</button>
    </form>
</p1>


@foreach (var user in Model)
{
    <div>
        <p>@user.NickName</p>
        <p>@user.Email</p>
        <form asp-controller="OrganizationRights" asp-action="AddUser"
           asp-route-id="@ViewData["OrganizationId"]" asp-route-userId="@user.Id">
            <input type="submit" value="Add User To Organization" class="btn btn-info" />
        </form>
    </div>
}


